{
  "manifest_version": 2,
  "name": "Red String NotePad",
  "version": "0.1",
  "background":{
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["dependencies/jquery-3.0.0.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "resources/icon.png"
  }
}
